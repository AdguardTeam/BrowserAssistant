{
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "jsx": "react",
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "skipLibCheck": true,
        "allowJs": true,
        "noImplicitAny": true,
        "experimentalDecorators": true,
        "resolveJsonModule": true,
        "typeRoots": [
            "./node_modules/@types",
            "./types"
        ]
    },
}
