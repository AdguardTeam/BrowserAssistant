{
    "manifest_version": 2,
    "name": "AdGuard extension boilerplate Dev",
    "short_name": "__MSG_short_name__",
    "author": "Adguard Software Ltd",
    "version": "0.0.3",
    "default_locale": "en",
    "description": "__MSG_description__",
    "icons": {
        "16": "assets/images/green-16.png",
        "128": "assets/images/green-128.png"
    },
    "browser_action": {
        "default_icon": {
            "19": "assets/images/green-19.png",
            "38": "assets/images/green-38.png"
        },
        "default_title": "__MSG_name__",
        "default_popup": "popup.html"
    },
    "background": {
        "page": "background.html"
    },
    "content_scripts": [
        {
            "all_frames": true,
            "js": [
                "content-scripts.js"
            ],
            "matches": [
                "http://*/*",
                "https://*/*"
            ],
            "match_about_blank": true,
            "run_at": "document_start"
        }
    ],
    "permissions": [
        "tabs",
        "http://*/*",
        "https://*/*",
        "webRequest",
        "webRequestBlocking",
        "webNavigation",
        "storage",
        "unlimitedStorage",
        "contextMenus",
        "cookies"
    ],
    "minimum_chrome_version": "55.0",
    "applications": {
        "gecko": {
            "id": "adguardadblockerdev@adguard.com",
            "strict_min_version": "52.0"
        }
    },
    "options_ui": {
        "page": "options.html",
        "open_in_tab": true
    },
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}