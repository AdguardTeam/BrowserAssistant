{
    "manifest_version": 2,
    "name": "__MSG_name__",
    "author": "Adguard Software Ltd",
    "version": "__version__",
    "default_locale": "en",
    "description": "__MSG_description__",
    "icons": {
        "16": "assets/images/assistant-logo_16.ico",
        "48": "assets/images/assistant-logo_48.png",
